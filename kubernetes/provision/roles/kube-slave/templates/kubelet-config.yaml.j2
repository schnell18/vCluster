kind: KubeletConfiguration
apiVersion: kubelet.config.k8s.io/v1beta1
authentication:
  anonymous:
    enabled: false
  webhook:
    enabled: true
  x509:
    clientCAFile: "{{ sys_share_ca_dir }}/ownca.crt"
authorization:
  mode: Webhook
clusterDomain: "cluster.local"
clusterDNS:
  - "10.32.0.10"
podCIDR: "{{ pod_cidr }}"
runtimeRequestTimeout: "15m"
tlsCertFile: "{{ kubelet_data_dir }}/{{ inventory_hostname }}.pem"
tlsPrivateKeyFile: "{{ kubelet_data_dir }}/{{ inventory_hostname }}-key.pem"